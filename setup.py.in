#!/usr/bin/python

from distutils.core import setup

setup (
        name = "slip",
        version = "@VERSION@",
        py_modules = ["slip.__init__"]
        )

setup (
        name = "slip.dbus",
        version = "@VERSION@",
        py_modules = [
            "slip.dbus.__init__",
            "slip.dbus.polkit",
            "slip.dbus.proxy",
            "slip.dbus.service"
            ]
        )

setup (
        name = "slip.gtk",
        version = "@VERSION@",
        py_modules = ["slip.gtk.__init__"]
        )
